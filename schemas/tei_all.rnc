  element ns2:triangle {
    (label?, (eTree | triangle | eLeaf)*),
    att.global.attributes,
    
    ## supplies a value for the triangle, in the form of the identifier of a feature structure or other analytic element.
    attribute value { data.pointer }?,
    empty
  }
eLeaf =
  
  ## (leaf or terminal node of an embedding tree) provides explicitly for a leaf of an embedding tree, which may also be encoded with the eTree element. [19.3. ]
  element ns2:eLeaf {
    (label?, model.ptrLike?),
    att.global.attributes,
    att.typed.attributes,
    
    ## indicates the value of an embedding leaf, which is a feature structure or other analytic element.
    attribute value { data.pointer }?,
    empty
  }
forest =
  
  ## provides for groups of rooted trees. [19.3. ]
  element ns2:forest {
    (tree | eTree | triangle)+,
    att.global.attributes,
    att.typed.attributes,
    empty
  }
listForest =
  
  ## provides for lists of forests. [19.3. ]
  element ns2:listForest {
